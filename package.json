{
  "name": "wingman",
  "description": "Wingman is a Twitter clone and middleman service that aims to provide users with a similar experience to the official Twitter platform while giving them more control over their data and interactions.",
  "version": "0.0.0",
  "devDependencies": {
    "@forvais/eslint-config-base": "^1.0.0",
    "@forvais/eslint-config-node": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  },
  "scripts": {
    "install": "./scripts/propagate.sh npm install",
    "lint": "./scripts/propagate.sh npm run lint",
    "lint:all": "./scripts/propagate.sh npm run lint:all",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "./scripts/propagate.sh npm run test:unit",
    "test:e2e": "./scripts/propagate.sh npm run test:e2e"
  }
}
